
@using WebApplication1.Models
@model IEnumerable<WebApplication1.Models.Employee>


@{
    ViewBag.Title = "Employee Data";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<h3>Employee Data</h3>
<table class="table table-condensed table-hover">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(e => e.ID)</th>
            <th>@Html.DisplayNameFor(e => e.Name)</th>
            <th>@Html.DisplayNameFor(e => e.Age)</th>
        
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in (IEnumerable<Employee>)TempData["EmployeeDetails"])
        {
            <tr>
                <td>@employee.ID</td>
                <td>@employee.Name</td>
                <td>@employee.Age</td>
                
            </tr>

        }
    </tbody>
</table>



